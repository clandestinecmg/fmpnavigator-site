{
  "name": "fmpnavigator-site",
  "version": "0.1.0",
  "private": true,
  "description": "FMP Navigator website â€“ Next.js App Router + Firebase + Google Maps",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "providers:enrich:ph": "tsx scripts/enrich-places.ts --country-bias PH --out data/providers.enriched.json",
    "providers:enrich:th": "tsx scripts/enrich-places.ts --country-bias TH --out data/providers.enriched.json",
    "providers:merge": "tsx scripts/merge-providers.ts --enriched data/providers.enriched.json --out data/providers.final.json",
    "providers:merge:geo": "tsx scripts/merge-providers.ts --enriched data/providers.enriched.json --out data/providers.final.json --overwrite-geo"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
